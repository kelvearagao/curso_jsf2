		if ( value != null )
		{
			Session session = (Session) HibernateUtil.getRequestAttribute("session");
		}
		return null;
	}

	@Override
	public String getAsString(FacesContext arg0, UIComponent arg1, Object arg2) {
		// TODO Auto-generated method stub
		return null;
	}

}
